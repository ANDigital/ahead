{
  "extends": ["airbnb-base", "plugin:security/recommended"],
  "plugins": ["security"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "script",
    "ecmaFeatures": {
      "jsx": false
    },
    "impliedStrict": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "import/no-extraneous-dependencies": "off"
  }
}